<pages>
    <!--定义一个page对象-->
    <page keyword="注册页面">
        <!--对应页面上的元素，keyword：页面元素关键字，by：定位方式，value：定位值-->
        <ele keyword="用户名" by="id" value="username"></ele>
        <ele keyword="密码" by="id" value="password"></ele>
        <ele keyword="重复密码" by="id" value="pwdconfirm"></ele>
        <ele keyword="注册" by="className" value="submit_btn"></ele>
        <ele keyword="错误提示" by="className" value="tips"></ele>
    </page>
    <page keyword="登陆页面">
        <!--对应页面上的元素，keyword：页面元素关键字，by：定位方式，value：定位值-->
        <ele keyword="用户名" by="name" value="username"></ele>
        <ele keyword="密码" by="name" value="password"></ele>
        <ele keyword="登陆" by="id" value="login"></ele>
        <ele keyword="错误提示" by="className" value="tips"></ele>
    </page>

    <page keyword="添加列表页面">
        <!--对应页面上的元素，keyword：页面元素关键字，by：定位方式，value：定位值-->
        <ele keyword="项目" by="className" value="item_single"></ele>
        <ele keyword="添加项目" by="className" value="className"></ele>
        <ele keyword="项目名称" by="name" value="name"></ele>
        <ele keyword="项目地址" by="name" value="host"></ele>
        <ele keyword="项目描述" by="name" value="description"></ele>
    </page>
</pages>